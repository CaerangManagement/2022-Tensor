# 1번 문제

from decimal import Decimal

a, b = map(int, input().split())

print("{:.20f}".format(Decimal(a) / Decimal(b)))

# 2번 문제

s = int(input())

n = 1
while n * (n+1) / 2 <= s: # 1부터 n까지의 합의 공식
    n += 1
print(n - 1) # n 번째를 구하면 입력한 숫자보다 커지므로 하나전까지

# 3번 문제

n = int(input())

li = []

for i in range(0, n):
    nums = map(int, input().split())
    li.append(nums)

#     for j in range(0, n):
#         re_li = [:n*j]
        
print(li)
